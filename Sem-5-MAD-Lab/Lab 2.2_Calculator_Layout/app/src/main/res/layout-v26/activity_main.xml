        android:layout_marginEnd="21dp"
        android:layout_marginRight="21dp"
        android:layout_marginBottom="16dp"
        android:text="@string/_5"
        app:layout_constraintBottom_toTopOf="@+id/B_Num2"
        app:layout_constraintEnd_toStartOf="@+id/B_Num6" />

    <Button
        android:id="@+id/B_Num0"
        android:layout_width="89dp"
        android:layout_height="49dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginEnd="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginBottom="27dp"
        android:text="@string/_0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/B_Num00"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/B_Num00"
        android:layout_width="89dp"
        android:layout_height="49dp"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginEnd="19dp"
        android:layout_marginRight="19dp"
        android:layout_marginBottom="24dp"
        android:text="@string/_00"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/b_Num."
        app:layout_constraintStart_toEndOf="@+id/B_Num0" />

    <Button
        android:id="@+id/b_Num."
        android:layout_width="89dp"
        android:layout_height="49dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:layout_marginBottom="24dp"
        android:text="@string/_Decimal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/B_Num00" />

